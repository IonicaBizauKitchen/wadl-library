        <resource path="addvenue.{format}">
            <param name="format" type="xsd:string" style="template" required="true" default="json">
                <option value="xml" mediaType="application/xml"/>
                <option value="json" mediaType="application/json"/>
            </param>
            <param name="name" required="true" type="xsd:string" style="query" default="Pig &amp; Whistle">
                <doc>The name of the venue</doc>
            </param>
            <param name="address" required="false" type="xsd:string" style="query" default="202 1st Avenue">
                <doc>(optional) the address of the venue</doc>
            </param>
            <param name="crossstreet" required="false" type="xsd:string" style="query" default="btw Grand &amp; Broome">
                <doc>(optional) the cross streets</doc>
            </param>
            <param name="city" required="false" type="xsd:string" style="query" default="Rome">
                <doc>(optional) the city name where this venue is</doc>
            </param>
            <param name="state" required="false" type="xsd:string" style="query" default="Italy">
                <doc>(optional) the state where the city is</doc>
            </param>
            <param name="zip" required="false" type="xsd:string" style="query" default="10002">
                <doc>(optional) the ZIP code for the venue</doc>
            </param>
            <param name="phone" required="false" type="xsd:string" style="query" default="2025551212">
                <doc>(optional) the phone number for the venue</doc>
            </param>
            <param name="geolat" required="false" type="xsd:string" style="query" default="40.01685">
                <doc>Geographical Latitude</doc>
            </param>
            <param name="geolong" required="false" type="xsd:string" style="query" default="-105.276489">
                <doc>Geographical Longitude</doc>
            </param>
            <param name="primarycategoryid" required="false" type="xsd:string" style="query" default="12345">
                <doc>(optional) the ID of the category to which you want to assign
                    this venue
                </doc>
            </param>
            <method id="add_venue" name="POST">
                <apigee:tags xmlns:apigee="http://api.apigee.com/wadl/2010/07/">
                    <apigee:tag primary="true">Venue</apigee:tag>
                </apigee:tags>
                <apigee:authentication xmlns:apigee="http://api.apigee.com/wadl/2010/07/" required="true"/>
                <apigee:example xmlns:apigee="http://api.apigee.com/wadl/2010/07/"
                                url="/addvenue.{format}?name={name}&amp;geolat={geolat}&amp;geolong={geolong}"/>
            </method>
        </resource>

